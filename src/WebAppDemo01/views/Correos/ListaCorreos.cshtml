@model ListaCorreosViewModel


@foreach (var correos in Model.Correos)
{
      
    <div class="col-lg-3 col-md-6 mb-4">
        <div class="card">
            <img class="card-img-top" src="@correos.ImagenPreviaURL" alt="">
            <div class="card-body">
                <h2 class="card-text">@correos.FechaIngreso</h2>
                <h4 class="card-title">@correos.NombreCorreo</h4>
                <p class="card-text">@correos.DescripCorreo</p>
                <p class="card-text">@correos.DescripLargaCorreo</p>
                <p class="card-text">@correos.EstadoCorreo</p>
            </div>
        </div>
    </div>
}